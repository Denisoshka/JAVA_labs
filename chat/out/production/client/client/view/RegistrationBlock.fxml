<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         prefHeight="60.0" prefWidth="180.0" type="VBox">
    <TextField fx:id="hostname" prefWidth="Infinity"/>
    <TextField fx:id="port" prefWidth="Infinity"/>
    <TextField fx:id="userLogin" prefWidth="Infinity"/>
    <PasswordField fx:id="userPassword" prefWidth="Infinity"/>
    <Button fx:id="login" text="login" prefWidth="Infinity"/>
    <Button fx:id="logout" text="logout" prefWidth="Infinity"/>
</fx:root>
