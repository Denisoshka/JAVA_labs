<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="javachat.client.view.RegistrationBlock"
         prefHeight="400.0" prefWidth="600.0" type="VBox">
    <TextField fx:id="chatName"/>
    <TextField fx:id="userLogin"/>
    <PasswordField fx:id="userPassword"/>
    <Button fx:id="login" onAction="#performLogin"/>
    <Button fx:id="logout" onAction="#performLogout"/>
</fx:root>
